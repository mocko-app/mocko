  "homepage": "https://github.com/mocko-app/mocko#readme",
  "devDependencies": {
    "@hapi/hapi": "^20.2.2",
    "@types/hapi__hapi": "^20.0.12",
    "@types/jest": "^27.0.0",
    "@types/node": "^15.14.9",
    "axios": "^0.21.4",
    "jest": "^27.0.4",
    "nodemon": "^3.1.7",
    "rimraf": "^3.0.2",
    "ts-jest": "^27.0.4",
    "typescript": "^4.8.2"
  },
