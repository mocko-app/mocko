  "description": "Logging utility used by Mocko.dev",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "prebuild": "rimraf lib",
    "build": "tsc",
    "test": "jest",
    "prepare": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mocko-app/mocko.git"
  },
  "keywords": [
    "mocko",
    "logging",
    "log",
    "mock",
    "stub"
