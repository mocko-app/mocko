{
  "name": "mocko-e2e",
  "version": "0.0.1",
  "description": "Mocko end-to-end tests",
  "main": "index.js",
  "scripts": {
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mocko-app/mocko.git"
  },
  "author": "Gabriel Pinheiro <gabrielctpinheiro@gmail.com>",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/mocko-app/mocko/issues"
  },
  "homepage": "https://github.com/mocko-app/mocko#readme",
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./tests",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "testEnvironment": "node"
  },
  "dependencies": {
    "@types/jest": "^28.1.8",
    "@types/node": "^16.0.0",
    "@types/uuid": "^8.3.4",
    "axios": "^0.27.2",
    "jest": "^28.0.0",
    "ts-jest": "^28.0.8",
    "typescript": "^4.8.2",
    "uuid": "^8.3.2"
  }
}
